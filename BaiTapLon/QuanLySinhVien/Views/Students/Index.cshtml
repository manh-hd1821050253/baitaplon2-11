@model IEnumerable<QuanLySinhVien.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>Mã Sinh Viên</th>
        <th>
            @Html.DisplayNameFor(model => model.NameStudent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.IDstudent)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.NameStudent)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.IDstudent }) |
        @Html.ActionLink("Details", "Details", new { id = item.IDstudent }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.IDstudent })
    </td>
</tr>
}

</table>
